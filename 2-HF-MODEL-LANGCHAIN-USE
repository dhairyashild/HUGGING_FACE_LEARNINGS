import os
from langchain_huggingface import ChatHuggingFace, HuggingFaceEndpoint

os.environ["HUGGINGFACEHUB_API_TOKEN"] = "hf_..."                                # WE DONT NEED THIS AS WE CALL         decouple + config            TO CALL KEY NAME

llm = HuggingFaceEndpoint(
    repo_id="microsoft/Phi-3-mini-4k-instruct",               # CHANGE THIS NAME TO RKING MODELS LIKE   HuggingFaceH4/zephyr-7b-beta   / mistralai/Mistral-7B-Instruct-v0.2 / meta-llama/Meta-Llama-3-8B-Instruct
    temperature=0.7,
    max_length=1024,                                          # max_new_tokens=1024,             REPLACE THIS INTO CODE AND REMOVE max_length=1024,
)
model = ChatHuggingFace(llm=llm)

                                                              # responce=model.invoke   + print(responce)                       after this add what u want                    






























